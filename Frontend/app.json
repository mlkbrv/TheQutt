{
  "expo": {
    "name": "Qutt",
    "slug": "qutt-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "userInterfaceStyle": "light",
    "splash": {
      "backgroundColor": "#2E7D32"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "infoPlist": {
        "NSLocationWhenInUseUsageDescription": "The app needs access to location to show nearby shops on the map",
        "NSLocationAlwaysAndWhenInUseUsageDescription": "The app needs access to location to show nearby shops on the map"
      }
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#2E7D32"
      },
      "permissions": [
        "ACCESS_FINE_LOCATION",
        "ACCESS_COARSE_LOCATION",
        "INTERNET"
      ],
      "usesCleartextTraffic": true,
      "package": "com.emil_a.quttappr"
    },
    "web": {},
    "extra": {
      "eas": {
        "projectId": "3e51f736-7569-4afb-85d2-78df00d0220c"
      }
    }
  }
}
